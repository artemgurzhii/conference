{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "source": {
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "templates": {
    "systemName": "Web Conferences",
    "footer": "<p class=\"copyright\" style=\"font: 300 1.3em sans-serif;letter-spacing: 0.1em;text-align: center;\">Copyright &copy; 2016 <a href=\"https://github.com/artemgurzhii\" style=\"color: #2ebaae;\"> Artem Gurzhii </a></p>",
    "theme": "spacelab",
    "navType": "inline",
    "collapseSymbols": true,
    "sort": false
  },
  "opts": {
    "destination": "./docs"
  }
}
